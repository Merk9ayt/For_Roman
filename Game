void screen_cave_with_torch_and_spiders()
{
	system("cls");
	cout << " ________________________________________________________________________________________________________________ \n";
	cout << "| ~~~~~~~~~~~~~~~~~~~~~~~~~|~~||||/////_/|//|/______________|~~||~~~~|~|~~~~~~~~~~~~~~~~~|~~~|||||||>>..         |\n";
	cout << "| ~||~~~~~~~~____________~~|////_/|||///|/|||(       |      )|||||||~~~||~~~~~~~~~~~~~~~|||~~||||||/>.>/         |\n";
	cout << "| //_>>||~~~(            )>>>_||||//|~||||||(     ---|---    )|||||||||||~~~~~|~~~~~~~~~|/~|||||||||>>//  ) | /  |\n";
	cout << "| .>__>>_>|(              )_>>___||~|||~|//(         |        )~|||||||~~~~~~~~________________|||||||// -- O -- |\n";
	cout << "| >>__.<..( (   ______   ) )~~/____/|/||~~( (     ______     ) )|||||~~~~~~~~~( (   ______   ) )|||||///  /| |)  |\n";
	cout << "| _/__//>/ ((  (0    0)  )) )~~~|___/||/|/ ((    (0    0)    )) )|||~~~~~~~~~/ ((  (0    0)  )) )|||////   | |   |\n";
	cout << "| ||||||/ / (( ('    ') )) ) )~~~~~|__/|/ / ((   ('    ')   )) ) )|~~~~~~~~~/ / (( ('    ') )) ) )|/////   | |   |\n";
	cout << "| |||||/ / /  ( |'  '| )  ) ) )_~~~~~~_/ / /  (   |'  '|   )  ) ) )~~~~~~~~/ / /  ( |'  '| )  ) ) )/////   | |   |\n";
	cout << "| |~||/ / /  /   ----   )  ) ) ).>/~~~/ / /  /     ----     )  ) ) )||~~~~/ / /  /   ----   )  ) ) )__/_   | |   |\n";
	cout << "| |||/ / /  /            )  ) ) )||_|/ / /  /                )  ) ) )~|_./ / /  /            )  ) ) )___   | |   |\n";
	cout << "| ||/ / /  /              )  ) ) )__/ / /  /                  )  ) ) )>_/ / /  /              )  ) ) )__   | |   |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}

