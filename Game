void screen_with_spider_2()
{
	system("cls");
	cout << " ________________________________________________________________________________________________________________ \n";
	cout << "|                                    | |>_|~~~/~~~~~~~~~~~~~~~~|~~~~|___| |                                      |\n";
	cout << "|                                    | |._/~~~~|~ ____________ ~|~~~~|__| |                                      |\n";
	cout << "|                                    | |>_/|~~~/ (            ) /~~~~|/_| |                               ) | /  |\n";
	cout << "|                                    | |>__|~~~ (              ) ~~~~|_>| |                              -- O -- |\n";
	cout << "|                                    | |/~.>_  ( (   ______   ) )  |~~~|| |                               /| |)  |\n";
	cout << "|                                    | |_||~~ / ((  (0    0)  )) ) |_>_|| |                                | |   |\n";
	cout << "|                                    | |~||| / / (( ('    ') )) ) ) ||||| |                                | |   |\n";
	cout << "|                                    | |||| / / /  ( |'  '| )  ) ) ) ||/| |                                | |   |\n";
	cout << "|                                    | |// / / /  /   ----   )  ) ) ) //| |                                | |   |\n";
	cout << "|                                    | |/ / / /  /            )  ) ) ) /| |                                | |   |\n";
	cout << "|                                    | | / / /  /              )  ) ) ) | |                                | |   |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}

