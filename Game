	cout << "|                                    | | (  (  (  )_____|_____/ )  )  ) | |                               ) | /  |\n";
	cout << "|                                    | |(  (  (  (  )___|___/ )  )  )  )| |                              -- O -- |\n";
	cout << "|                                    | |( (  (  (   ( )_|_/ )  )  )  ) )| |                               /| |)  |\n";
	cout << "|                                    | |----------------|---------------| |                                | |   |\n";
	cout << "|                                    | |( (  (  (   ( /_|_)  )  )  )  ) | |                                | |   |\n";
	cout << "|                                    | |(  (  (  (  /___|___)  )  )  ) )| |                                | |   |\n";
	cout << "|                                    | | (  (  (  /_____|_____)  )  ) ) | |                                | |   |\n";
	cout << "|                                    | |  (  (  /_______|_______)  ) ) )| |                                | |   |\n";
	cout << "|                                    | |   (  /_________|_________) ) ) | |                                | |   |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}

