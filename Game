void screen_with_spider_1()
{
	system("cls");
	cout << " ________________________________________________________________________________________________________________ \n";
	cout << "|                                    | |>_|~~~/~~~~~~~~~~~~~~~~|~~~~|___| |                                      |\n";
	cout << "|                                    | |._/~~~~|~~~~~~~~~~~~~~~~|~~~~|__| |                                      |\n";
	cout << "|                                    | |>_/|~~~/|~~~~~~~~~~~~~~~/~~~~|/_| |                               ) | /  |\n";
	cout << "|                                    | |>__|~~~ ________________ ~~~~|_>| |                              -- O -- |\n";
	cout << "|                                    | |/~.>_  ( (   ______   ) )  |~~~|| |                               /| |)  |\n";
	cout << "|                                    | |_||~~ / ((  (0    0)  )) ) |_>_|| |                                | |   |\n";
	cout << "|                                    | |~||| / / (( ('    ') )) ) ) ||||| |                                | |   |\n";
	cout << "|                                    | |||| / / /  ( |'  '| )  ) ) ) ||/| |                                | |   |\n";
	cout << "|                                    | |// / / /  /   ----   )  ) ) ) //| |                                | |   |\n";
	cout << "|                                    | |/ / / /  /            )  ) ) ) /| |                                | |   |\n";
	cout << "|                                    | | / / /  /              )  ) ) ) | |                                | |   |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}

