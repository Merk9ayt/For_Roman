	cout << "| //_>>||~~~~~~~~~~~~||/>>>>>_||||//|~||||||///           )||||||||||||||~~~~~|~~~~~~~~~|/~|||||||||>>//  ) | /  |\n";
	cout << "| .>__>>_>||~~~~~/||_>/~~|/_>>___||~|||~|//|||             ||~~~|||||||~~~~~~~~~~~~~~~~|||_/||||||||||// -- O -- |\n";
	cout << "| >>__.<..>_|||||/<>_|/_|~~~~~/____/|/||~~~|/|             ||~|||||||~~~~~~~~~~~~~~~~~~||//||||||||||///  /| |)  |\n";
	cout << "| _/__//>__/>_.>/___>>/|_./~~~~~|___/||/|~~~||           Q |||||||||~~~~~~~~~~~~~~~~~|||||/|||||||||////   | |   |\n";
	cout << "| ||||||__.>/|_...>//_<//|>._|~~~~~|__/||||~~|_____________||||||/|~~~~~~~~~~~~~~~~~~~~~||//|||||||/////   | |   |\n";
	cout << "| ||||||||_></_>,,,<_||||/_<.//_~~~~~~___///||_||_______||_|~~~||/|~~~~~~~~~~~~~~~~~~~~~~||/____////////   | |   |\n";
	cout << "| |~||||||||||_><,,<>_/||||_.._|.>/~~~~|__///|             |/|||||~~~~~~~~~|/___//|/||/_____//////____/_   | |   |\n";
