void sсreen_door()
{
	system("cls");
	cout << " ________________________________________________________________________________________________________________ \n";
	cout << "|                                            _________                                                           |\n";
	cout << "| ~   ~            ~         ~       ~      /         )                  ~              ~                        |\n";
	cout << "| / /  /                                   /   __      )      /                                    /       /     |\n";
	cout << "|     /                                   |   |XX)      |           /                                            |\n";
	cout << "|                     /                   |   |mm|      |                                                        |\n";
	cout << "|                          /              |    --     Q |                    ~        ~~~~~~~~~~      /          |\n";
	cout << "|                            /            |_____________|                                                        |\n";
	cout << "|         /          /                    |_||_______||_|                 /           /                          |\n";
	cout << "|                                         |             |                            /               /           |\n";
	cout << "|          /         /  /                 |             |                            /                           |\n";
	cout << "|      /                          /       |_____________|                         /               /              |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}

