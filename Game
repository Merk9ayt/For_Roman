void passage_with_door()
{
	system("cls");
	cout << " ________________________________________________________________________________________________________________ \n";
	cout << "|~~~~~~~~~~~~~~~~~~|~~||||/////_/|//|| |>_|~~~/~~~~~~~~~~~~~~~~|~~~~|___| |||||||||~~~||~~~~~~~~~~~~~~~|         |\n";
	cout << "|~~~~~~~~~~~~~~~~~~|////_/|||///|/|||| |._/~~~~|~~~~~~~~~~~~~~~~|~~~~|__| ||||||||||||||~~~~~|~~~~~~~~~|         |\n";
	cout << "|~~~~~~~~~~~~||/>>>>>_||||//|~||||||/| |>_/|~~~/|~~~~~~~~~~~~~~~/~~~~|/_| ||~~~|||||||~~~~~~~~~~~~~~~~||  ) | /  |\n";
	cout << "|>||~~~~~/||_>/~~|/_>>___||~|||~|//||| |>__|~~~|~~~~~~~~~~~~~~~~/~~~~|_>| ||~|||||||~~~~~~~~~~~~~~~~~~|| -- O -- |\n";
	cout << "|.>_|||||/<>_|/_|~~~~~/____/|/||~~~|/| |/~.>_|~~~||~~~~~~~~~~~~~~~/|~~~|| |||||||||~~~~~~~~~~~~~~~~~||||  /| |)  |\n";
	cout << "|__/>_.>/___>>/|_./~~~~~|___/||/|~~~|| |_||~~||~~~~||||~~~~~~||~~~~|_>_|| ||||||/|~~~~~~~~~~~~~~~~~~~~~|   | |   |\n";
	cout << "|_.>/|_...>//_<//|>._|~~~~~|__/||||~~| |~|||||||||~||||||||||~~~|~~|||||| |~~~||/|~~~~~~~~~~~~~~~~~~~~~~   | |   |\n";
	cout << "||_></_>,,,<_||||/_<.//_~~~~~~___///|| |||||||||||///////|||||~||||||||/| ||/||/|~|_.<<<.....>>_//_/////   | |   |\n";
	cout << "||||||_><,,<>_/||||_.._|.>/~~~~|__///| |///////|//////|||||//|||////|///| |/|||||~~~~~~~~~|/___//|/||/__   | |   |\n";
	cout << "|||||||||/_.._/||||_|_>>>||_|~~||~|/|| |////////////////////////////////| ||/||/|~|_.<<<.....>>_//_/////   | |   |\n";
	cout << "|||||||/>..<../>/|/__>>>>___/_>_//|~|| |////////////////////////////////| |//>...>__>>___...>>>__/||~~|/   | |   |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}

