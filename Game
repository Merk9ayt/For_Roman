#include <iostream>
#include <string>
#include <ctime>

using namespace std;

void sсreen()
{
	system("cls");
	cout << " ________________________________________________________________________________________________________________ \n";
	cout << "|                                                                                                                |\n";
	cout << "|                                                                                                                |\n";
	cout << "|                                                                                                                |\n";      //Инта{INT}(очки,оружие:одноручные)[в ответе за умные решения]-крит.шанc
	cout << "|                                                                                                                |\n";      //Решимость{COU}(очки,оружие:двуручные)[в ответе за силовые решения]-наносимый урон
	cout << "|                                                                                                                |\n";      //HP-Здоровье
	cout << "|                                                                                                                |\n";      //Сопротивление{STR}(броня)-меньше получать урона
	cout << "|                                                                                                                |\n";
	cout << "|                                                                                                                |\n";
	cout << "|                                                                                                                |\n";
	cout << "|                                                                                                                |\n";
	cout << "|                                                                                                                |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}

void sсreen_dwarfs()
{
	system("cls");
	cout << " ________________________________________________________________________________________________________________ \n";
	cout << "|                                             (______)                                                           |\n";
	cout << "|                                            (        )                                                          |\n";
	cout << "|                                          (  ///   /// )                                                        |\n";      
	cout << "|                                         (   (*)    (*) )                                                       |\n";      
	cout << "|                                       ((         ))     ))                                                     |\n";      
	cout << "|                                         (     %%%%%    )                                                       |\n";      
	cout << "|                                         % ( % _____ % ) %                                                      |\n";
	cout << "|                                          % ( %%%%%%% ) %                                                       |\n";
	cout << "|                                   ________%  %%%%%%%   %________                                               |\n";
	cout << "|                                 (         % %%%%%%%%%  %         )                                             |\n";
	cout << "|                                (            %%%%%%%%%             )                                            |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}

