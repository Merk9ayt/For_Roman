	cout << "| |___/ /           ( )_/ /       |O|        (X)|#| |ll;;l;;l;;| | |______     (/ . |   )/    | | )        (/)| |\n";
	cout << "|______/             (___/        |_|         (___| |__________| |________|     (_______/     |_|           (___|\n";
	cout << "\n Спасибо за прохождение данной игры надеюсь вам понравилось.\n";
	system("pause");
}
