void screen_cave()
{
	system("cls");
	cout << " ________________________________________________________________________________________________________________ \n";
	cout << "| ~~~~~~~~~~~~~~~~~~~~~~~~~|~~||||/////_/|//|/_//____/////|/|~~||~~~~|~|~~~~~~~~~~~~~~~~~|~~~|||||||>>..         |\n";
	cout << "| ~||~~~~~~~~~~~~~~~~~~~~~~|////_/|||///|/|||/////_>>_////||||||||||~~~||~~~~~~~~~~~~~~~|||~~||||||/>.>/         |\n";
	cout << "| //_>>||~~~~~~~~~~~~||/>>>>>_||||//|~||||||///////___/|//|||||||||||||||~~~~~|~~~~~~~~~|/~|||||||||>>//         |\n";
	cout << "| .>__>>_>||~~~~~/||_>/~~|/_>>___||~|||~|//||//////////|//|/|~~~|||||||~~~~~~~~~~~~~~~~|||_/||||||||||//         |\n";
	cout << "| >>__.<..>_|||||/<>_|/_|~~~~~/____/|/||~~~|////||||||~|//|||~|||||||~~~~~~~~~~~~~~~~~~||//||||||||||///         |\n";
	cout << "| _/__//>__/>_.>/___>>/|_./~~~~~|___/||/|~~~|//|/|||||~|~|||||||||||~~~~~~~~~~~~~~~~~|||||/|||||||||////         |\n";
	cout << "| ||||||__.>/|_...>//_<//|>._|~~~~~|__/||||~~|///||||||||~~~|||||/|~~~~~~~~~~~~~~~~~~~~~||//|||||||/////         |\n";
	cout << "| ||||||||_></_>,,,<_||||/_<.//_~~~~~~___///|~~~~/|/||||||||~~~||/|~~~~~~~~~~~~~~~~~~~~~~||/____////////         |\n";
	cout << "| |~||||||||||_><,,<>_/||||_.._|.>/~~~~|__////|~~~~|/|||||||/|||||~~~~~~~~~|/___//|/||/_____//////____/_         |\n";
	cout << "| |||||||||||||||/_.._/||||_|_>>>||_|~~||~|/|///|~~~~||||||||/||/|~|_.<<<.....>>_//_////////__///_______         |\n";
	cout << "| |||||||||||||/>..<../>/|/__>>>>___/_>_//|~|~~~||~|/||||/////>...>__>>___...>>>__/||~~|//||||||__>>>>__         |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}


void screen_cave_with_torch()
{
	system("cls");
	cout << " ________________________________________________________________________________________________________________ \n";
	cout << "| ~~~~~~~~~~~~~~~~~~~~~~~~~|~~||||/////_/|//|/_//____/////|/|~~||~~~~|~|~~~~~~~~~~~~~~~~~|~~~|||||||>>..         |\n";
	cout << "| ~||~~~~~~~~~~~~~~~~~~~~~~|////_/|||///|/|||/////_>>_////||||||||||~~~||~~~~~~~~~~~~~~~|||~~||||||/>.>/         |\n";
	cout << "| //_>>||~~~~~~~~~~~~||/>>>>>_||||//|~||||||///////___/|//|||||||||||||||~~~~~|~~~~~~~~~|/~|||||||||>>//  ) | /  |\n";
	cout << "| .>__>>_>||~~~~~/||_>/~~|/_>>___||~|||~|//||//////////|//|/|~~~|||||||~~~~~~~~~~~~~~~~|||_/||||||||||// -- O -- |\n";
	cout << "| >>__.<..>_|||||/<>_|/_|~~~~~/____/|/||~~~|////||||||~|//|||~|||||||~~~~~~~~~~~~~~~~~~||//||||||||||///  /| |)  |\n";
	cout << "| _/__//>__/>_.>/___>>/|_./~~~~~|___/||/|~~~|//|/|||||~|~|||||||||||~~~~~~~~~~~~~~~~~|||||/|||||||||////   | |   |\n";
	cout << "| ||||||__.>/|_...>//_<//|>._|~~~~~|__/||||~~|///||||||||~~~|||||/|~~~~~~~~~~~~~~~~~~~~~||//|||||||/////   | |   |\n";
	cout << "| ||||||||_></_>,,,<_||||/_<.//_~~~~~~___///|~~~~/|/||||||||~~~||/|~~~~~~~~~~~~~~~~~~~~~~||/____////////   | |   |\n";
	cout << "| |~||||||||||_><,,<>_/||||_.._|.>/~~~~|__////|~~~~|/|||||||/|||||~~~~~~~~~|/___//|/||/_____//////____/_   | |   |\n";
	cout << "| |||||||||||||||/_.._/||||_|_>>>||_|~~||~|/|///|~~~~||||||||/||/|~|_.<<<.....>>_//_////////__///_______   | |   |\n";
	cout << "| |||||||||||||/>..<../>/|/__>>>>___/_>_//|~|~~~||~|/||||/////>...>__>>___...>>>__/||~~|//||||||__>>>>__   | |   |\n";
	cout << "|________________________________________________________________________________________________________________|\n";
}

